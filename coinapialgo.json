{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",

  "description": "",

   "title": {
    "text": "Algorand: 10DAY Average Price",
    "subtitle":"From July-19. Source: coinapi.io",
    "subtitleFontStyle":"italic",
    "subtitleFontSize":10,
    "anchor": "start",
    "color": "#000000",
    "subtitleColor":"#a0db17"},

  "config": {
    "background":"#ffffff",
    "view": {
      "stroke": "transparent"}},
 
  
  "data": {
    "url": "https://rest.coinapi.io/v1/exchangerate/ALGO/USD/history?apikey=AAB3A57B-7D91-4EA7-9C8C-6211E2F7CEDF&period_id=10DAY&time_start=2019-07-01T00:00:00&time_end=2022-02-01T00:00:00&limit=100",
    "format": {"type": "json"}},
  

  "height": 300,
  "width": 280,
  

  "mark": {
    "type": "line",
    "point": false,
    "interpolate": "linear",
    "color":"#f46e45"
  },

  "encoding": {
    "x": {
      "field": "time_period_end",
      "type": "temporal",
      "axis": {
        "title": "Date",
        "grid": true,
        "labelSeparation": 10,
        "domainColor":"dimgrey",
        "labelColor":"#1f00a7",
        "tickColor":"black",
        "gridColor":"dimgrey"
      }},

    "y": {
      "field": "rate_open",
      "type": "quantitative",
      "title": "",
      "axis": {
        "grid": true,
        "domainColor":"dimgrey",
        "labelColor":"#1f00a7",
        "tickColor":"black",
        "gridColor":"dimgrey",
        "format":"$"}
        }
     }
}
